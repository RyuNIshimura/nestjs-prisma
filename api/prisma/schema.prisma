datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                   Int       @id @default(autoincrement())
  email                String    @unique
  auth0Sub             String    @unique
  stripeCustomerId     String?
  stripeCardBrand      String?
  stripeCardLastFour   String?
}
